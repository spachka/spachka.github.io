<script>
    let isRotated = false; // Переменная для отслеживания состояния вращения

    function toggleRotation() {
        const image = document.querySelector('.rotated-image1');
        isRotated = !isRotated; // Переключаем состояние
        if (isRotated) {
            image.classList.add('rotated'); // Добавляем класс вращения
        } else {
            image.classList.remove('rotated'); // Убираем класс вращения
        }
    }
    

    
</script>

